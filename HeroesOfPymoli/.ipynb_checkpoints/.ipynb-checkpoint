    "#Create new data frame that gets rid of duplicate SN\n",
    "unique_players = purchase_data.drop_duplicates(\"SN\")\n",
    "#Splice data so the gender columns is only male\n",
    "male = unique_players.loc[unique_players[\"Gender\"]== \"Male\",:]\n",
    "#Find the count of male players\n",
    "count_male= len(male)\n",
    "#Find percentage of male players\n",
    "percent_male = (count_male/total_players)*100\n",
    "#Give percent male the right format\n",
    "percent_male = \"{:,.2f}%\".format(percent_male)\n",
    "female = unique_players.loc[unique_players[\"Gender\"]== \"Female\",:]\n",
    "count_female= len(female)\n",
    "percent_female = (count_female/total_players)*100\n",
    "percent_female = \"{:,.2f}%\".format(percent_female)\n",
    "other = unique_players.loc[unique_players[\"Gender\"]== \"Other / Non-Disclosed\",:]\n",
    "count_other= len(other)\n",
    "percent_other = (count_other/total_players)*100\n",
    "percent_other = \"{:,.2f}%\".format(percent_other)\n",
    "#Create summary data frame\n",
    "sum_gender = pd.DataFrame({\"Total Count\": [count_male, count_female, count_other],\n",
    "                           \"Percentage of Players\": [percent_male, percent_female,percent_other]}, \n",
    "                         index = ['Male', 'Female', 'Other / Non-Disclosed'])\n",
    "                           \n",
    "sum_gender"

   "source": [
    "#Establish bins for ages\n",
    "bins = [0,9,14,19,24,29,34,39,100]\n",
    "age_labels = [\"<10\",\"10-14\",\"15-19\",\"20-24\",\"25-29\",\"30-34\",\"35-39\",\"40+\"]\n",
    "#Categorize the existing players using age bins, no duplicate SN\n",
    "unique_players[\"Age Group\"] = pd.cut(unique_players[\"Age\"],bins,labels=age_labels)\n",
    "#Group the dataframe by Age Group \"bin\" to perform calculations\n",
    "grouped = unique_players.groupby(\"Age Group\")\n",
    "#Count total number of players in each age group\n",
    "count_age = grouped[\"Purchase ID\"].count()\n",
    "#Finds the percent of players in each age group\n",
    "percent = ((count_age/total_players)*100).map(\"{:,.2f}%\".format)\n",
    "#Create summary data frame\n",
    "age_demo = pd.DataFrame({\"Total Count\": count_age,\n",
    "                        \"Percentage of Players\": percent})\n",
    "age_demo"
   ]
  },

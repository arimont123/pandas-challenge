    "#Calculate total spent by SN \n",
    "total_purch_val = (grouped_SN[\"Price\"].sum())\n",
    "#Create summary data frame\n",
    "top_spenders = pd.DataFrame({\"Purchase Count\": purchases,\n",
    "                               \"Avg. Purchase Price\": average_price,\n",
    "                               \"Total Purchase Value\": total_purch_val})\n",
    "#Sort the summary data frame largest to smallest by the total amount spent \n",
    "sorted_value = top_spenders.sort_values(\"Total Purchase Value\", ascending = False)\n",
    "sorted_value"

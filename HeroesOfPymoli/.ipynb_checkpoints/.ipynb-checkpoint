   "source": [
    "#Making calculations per gender out of entire dataset, contains duplicate SN \n",
    "#Splice data per gender\n",
    "fem = purchase_data.loc[purchase_data[\"Gender\"]== \"Female\",:]\n",
    "purchase_fem = len(fem)\n",
    "man = purchase_data.loc[purchase_data[\"Gender\"]== \"Male\",:]\n",
    "purchase_man = len(man)\n",
    "oth = purchase_data.loc[purchase_data[\"Gender\"]== \"Other / Non-Disclosed\",:]\n",
    "purchase_oth = len(oth)\n",
    "#avg purchase price per gender\n",
    "fem_price = fem[\"Price\"].mean()\n",
    "fem_price = \"${0:.2f}\".format(fem_price)\n",
    "man_price = man[\"Price\"].mean()\n",
    "man_price = \"${0:.2f}\".format(man_price)\n",
    "oth_price = oth[\"Price\"].mean()\n",
    "oth_price = \"${0:.2f}\".format(oth_price)\n",
    "#purchase total per gender\n",
    "fem_sum_price = fem[\"Price\"].sum()\n",
    "fem_sum = \"${:,.2f}\".format(fem_sum_price)\n",
    "man_sum_price = man[\"Price\"].sum()\n",
    "man_sum = \"${:,.2f}\".format(man_sum_price)\n",
    "oth_sum_price = oth[\"Price\"].sum()\n",
    "oth_sum = \"${:,.2f}\".format(oth_sum_price)\n",
    "#avg purchase total per gender\n",
    "avg_fem = fem_sum_price/count_female\n",
    "avg_fem = \"${0:.2f}\".format(avg_fem)\n",
    "avg_man = man_sum_price/count_male\n",
    "avg_man = \"${0:.2f}\".format(avg_man)\n",
    "avg_oth = oth_sum_price/count_other\n",
    "avg_oth = \"${0:.2f}\".format(avg_oth)\n",
    "#Create summary dataframe \n",
    "gender_analysis = pd.DataFrame({\"Purchase Count\": [purchase_fem,purchase_man,purchase_oth],\n",
    "                               \"Avg. Purchase Price\":[fem_price, man_price,oth_price],\n",
    "                               \"Total Purchase Value\":[fem_sum, man_sum,oth_sum],\n",
    "                               \"Avg. Total Purchase per Person\": [avg_fem, avg_man, avg_oth]},\n",
    "                              index = ['Female', 'Male', 'Other / Non-Disclosed'])\n",
    "gender_analysis"
   ]

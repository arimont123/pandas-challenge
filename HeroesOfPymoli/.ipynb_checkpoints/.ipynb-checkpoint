   "source": [
    "#Create bins for data\n",
    "bins = [0,9,14,19,24,29,34,39,100]\n",
    "age_labels = [\"<10\",\"10-14\",\"15-19\",\"20-24\",\"25-29\",\"30-34\",\"35-39\",\"40+\"]\n",
    "##Categorize all players using age bins, contains duplicate SN\n",
    "purchase_data[\"Age Group\"] = pd.cut(purchase_data[\"Age\"],bins,labels=age_labels)\n",
    "#Group the data frame by Age Group \"bin\"\n",
    "grouped_df = purchase_data.groupby(\"Age Group\")\n",
    "#Count how many purchases per age group\n",
    "purch_count = grouped_df[\"Purchase ID\"].count()\n",
    "#Calculates average price per age group\n",
    "avg_price_age = (grouped_df[\"Price\"].mean()).map(\"${:,.2f}\".format)\n",
    "#Calculates total purchases per age group\n",
    "total_val = (grouped_df[\"Price\"].sum())\n",
    "#Calculates average total purchase per person\n",
    "avg_purch_age = (total_val/count_age).map(\"${:,.2f}\".format)\n",
    "#Create summary data frame\n",
    "purch_analysis = pd.DataFrame({\"Purchase Count\": purch_count,\n",
    "                               \"Avg. Purchase Price\": avg_price_age,\n",
    "                               \"Total Purchase Value\":total_val,\n",
    "                               \"Avg. Total Purchase per Person\": avg_purch_age})\n",
    "purch_analysis"
   ]
  },

   "source": [
    "#Pull Item ID, Item Name, and Price out of original data frame\n",
    "item_data = purchase_data[[\"Item ID\", \"Item Name\", \"Price\"]]\n",
    "#First group by Item ID and then by Item Name\n",
    "grouped_cols = item_data.groupby([\"Item ID\", \"Item Name\"])\n",
    "#Calculate amount each item was purchased\n",
    "item_count = grouped_cols[\"Item ID\"].count()\n",
    "#Calculate average item price\n",
    "item_price = (grouped_cols[\"Price\"].mean())\n",
    "#Calculate total purchased per item \n",
    "tot_val = (grouped_cols[\"Price\"].sum())\n",
    "#Create summary dataframe \n",
    "popular_items = pd.DataFrame({\"Purchase Count\": item_count,\n",
    "                               \"Item Price\": item_price,\n",
    "                               \"Total Purchase Value\": tot_val})\n",
    "#Sort summary data frame by most purchased item, greatest to least\n",
    "sorted_items = popular_items.sort_values(\"Purchase Count\", ascending = False)\n",
    "sorted_items"
   ]
